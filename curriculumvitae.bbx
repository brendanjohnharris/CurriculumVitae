% FUNDING
\DeclareBibliographyDriver{funding}{%
\usebibmacro{bibindex}%
\usebibmacro{begentry}%
\vspace{-2em}
	\begin{entrylist}
		% ? Awards
		\IfSubStr{\thefield{keywords}}{Award}{%
			\entry
			{\usebibmacro{yearrange}}
			{\thefield{title}}
			{\printfield{type}}
			{%
			\iffieldundef{funder}{\thefield{note}}{\textit{\thefield{funder}}\\\thefield{note}}
			}
		}

		% ? Research projects
		\IfSubStr{\thefield{keywords}}{Research}{%
			\entry
			{\usebibmacro{yearrange}}
			{\thefield{title}
			\iffieldundef{eprint}{}{\quad \href{\thefield{eprint}}{\OA}}
			\iffieldundef{doi}{}{\quad \href{https://doi.org/\thefield{doi}}{\OA}}
			\iffieldundef{url}{}{\ \href{\thefield{url}}{\large \faicon{github}}
			}}
			{\printfield{role}}
			{%
				\chainit{funder}{%
				\iffieldundef{amount}{\thefield{note}}{
				\chain{note}{\\\thefield{amount} \thefield{currency}}}
				}
			}
		}

		% ? Industry projects
		\IfSubStr{\thefield{keywords}}{Industry}{%
			\entry
			{\usebibmacro{yearrange}}
			{\thefield{title}
			\iffieldundef{crossref}{}{\quad \href{\thefield{url}}{\OA}}
			\iffieldundef{eprint}{}{\ \href{\thefield{eprint}}{\large \faicon{github}}}
			}
			{\printfield{role}}
			{%
				\chainit{funder}{%
				\chain{note}{\\\thefield{amount} \thefield{currency}}}
			}
		}

		% ? Teaching
		\IfSubStr{\thefield{keywords}}{Teaching}{%
			\entry
			{\usebibmacro{yearrange}}
			{\thefield{title}
			\iffieldundef{crossref}{}{\quad \href{\thefield{url}}{\OA}}
			\iffieldundef{eprint}{}{\ \href{\thefield{eprint}}{\large \faicon{github}}}
			}
			{\printfield{type}}
			{%
				\chainit{funder}{\thefield{role}}
			}
		}\\
	\end{entrylist}
}

% Software
\def\githubstars#1{%
	% \newcommand{\stargazers}{}
	% \stargazers{}
	\def\stars{\input{|curl \-\-silent https://api.github.com/repos/\thefield{title} -H 'Accept: application/vnd.github.preview' | jq '.stargazers_count'}}
	\IfInteger{\string \stars}{%
		~\\{\tiny \faicon{star}\ \ }{\stars}
	}{}
}
\DeclareBibliographyDriver{software}{%
\usebibmacro{bibindex}%
\usebibmacro{begentry}%
\vspace{-2em}
	\begin{entrylist}
	\entry
	{\logo{eprint}}
	{%
		\thefield{title}
		\iffieldundef{url}{}{\quad \href{\thefield{url}}{\large \faicon{github}}}
	}
	{\begin{minipage}{0.1\textwidth}
		\begin{flushright}
			\iffieldundef{doi}{\printlist{publisher}}{%
				\textcolor{blue}{\href{https://doi.org/\thefield{doi}}{\printlist{publisher}}}%
			}%
			\githubstars{\thefield{title}}
		\end{flushright}
	\end{minipage}}
	{%
		\printfield{abstract}
	}\\
	\end{entrylist}
}

