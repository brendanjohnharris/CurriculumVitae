\newcommand{\NumToMonth}[1]{%
  \ifcase#1 % case 0, which should not occur
  \or Jan.% 1
  \or Feb.% 2
  \or Mar.% 3
  \or Apr.% 4
  \or May % 5
  \or Jun.% 6
  \or Jul.% 7
  \or Aug.% 8
  \or Sep.% 9
  \or Oct.% 10
  \or Nov.% 11
  \or Dec.% 12
  \else Invalid% other input
  \fi
}

\renewbibmacro{yearrange}{%
	\iffieldsequal{year}{endyear}{%
		\iffieldundef{month}{}{\NumToMonth{\thefield{month}}}
		\iffieldundef{endmonth}{%
			\printfield{year}%
		}{\bibrangedash\ \iffieldundef{endmonth}{}{\NumToMonth{\thefield{endmonth}}\ }\printfield{year}}
	}
	{%
		\iffieldundef{month}{}{\NumToMonth{\thefield{month}}\ }\printfield{year}%
		\ifboolexpr{%
			test {\iffieldundef{endyear}} or%
			test {\iffieldsequal{year}{endyear}}%
		}{}{\ \bibrangedash\ \iffieldundef{endmonth}{}{\\\NumToMonth{\thefield{endmonth}}\ }\printfield{endyear}}%
	}
}

