\def\OA{\includegraphics[trim=0 4.7cm 0 0cm, height=1.1em]{open-access-logo-color.pdf}}
\def\logo#1{%
\iffieldequalstr{#1}{Julia}{\includegraphics[trim=0 1cm 0 -1cm, height=1.2em]{julia-logo-color.pdf}}{}
\iffieldequalstr{#1}{MATLAB}{\raisebox{-1em}{\includegraphics[trim=0 1cm 0 -1cm, height=2.6em]{matlab-logo-color.png}}}{}
}

\def\chain#1#2{{\thefield{#1}\iffieldundef{#1}{#2}{}}}
\def\chainit#1#2{{\textit{\thefield{#1}}\iffieldundef{#1}{#2}{%
	\ifthenelse{\equal{#2}{}}{}{\\#2}
}}}

