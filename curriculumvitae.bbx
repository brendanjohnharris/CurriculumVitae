% ? Articles
\DeclareBibliographyDriver{article}{%
\usebibmacro{bibindex}%
\usebibmacro{begentry}%
\vspace{-2em}
\begin{entrylist}
	\entry
	{\usebibmacro{yearrange}}
	{\thefield{title}
		\iffieldundef{url}{}{\quad \href{\thefield{url}}{\OA}}
		\iffieldundef{note}{}{\ \href{\thefield{note}}{\large \faicon{github}}}
	}
	{\begin{minipage}[t]{0.15\textwidth}
		\iffieldundef{doi}{\thefield{journaltitle}}{\href{https://www.doi.org/\thefield{doi}}{\textcolor{blue}{\thefield{journaltitle}}}}
		\iffieldundef{journaltitle}{}{\\\bibstring{volume}\ \thefield{volume}, \bibstring{number} \thefield{number}}
	\end{minipage}
	}
	{\textit{\printnames{author}}
	}\\
	\end{entrylist}
}

% ? Abstracts
\DeclareBibliographyDriver{inproceedings}{%
\usebibmacro{bibindex}%
\usebibmacro{begentry}%
\vspace{-2em}
\begin{entrylist}
	\entry
	{\usebibmacro{yearrange}}
	{\thefield{booktitle}
		\iffieldundef{journaltitle}{}{\\\bibstring{volume}\ \thefield{volume}, \bibstring{number} \thefield{number}}
		\iffieldundef{url}{}{\quad \href{\thefield{url}}{\OA}}
		% \iffieldundef{note}{}{\ \href{\thefield{note}}{\large \faicon{github}}}
	}
	{\iffieldundef{doi}{\thefield{note}}{\href{https://www.doi.org/\thefield{doi}}{\textcolor{blue}{\thefield{note}}}} % The type of paper
	}{%
	\iffieldundef{pages}{}{\thefield{pages}:\ }\chainit{title}{\printnames{author}}
	}\\
	\end{entrylist}
}




